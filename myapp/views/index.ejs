              <div class="card-body">
                <h5 class="card-title">Sunglass</h5>
                <p class="card-text">ALL kinds of Glasses</p>
              </div>
            </div>
          </div>
          <div class="col-lg-3 col-6">
              <div class="card w-100">
                <div class="card-body">
                  <h5 class="card-title">Bags</h5>
                  <p class="card-text">All kind of Bags</p>
                </div>
              </div>
          </div>
          <div class="col-lg-3 col-6">
            <div class="card w-100">
              <div class="card-body">
                <h5 class="card-title">Table Lamps</h5>
                <p class="card-text">All veriety of Lamps</p>
              </div>
            </div>
          </div>
        </div>
      </div>
     </div>
    <%- include('./partials/footer'); %>
